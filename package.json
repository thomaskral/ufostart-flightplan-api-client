{
  "name": "ufostart-flightplan-api-client",
  "description": "API client for the UFOstart flightplan",
  "author": "Thomas Kral",
  "license": "MIT",
  "version": "0.0.11",
  "keywords": [
    "UFOstart",
    "flightplan",
    "marketing plan"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/thomaskral/ufostart-flightplan-api-client"
  },
